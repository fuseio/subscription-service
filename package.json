{
  "name": "fuse-subscription-service",
  "version": "0.1.0",
  "description": "node-skeleton",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/server.ts",
    "debug": "node --inspect-brk --trace-warnings ./src/app.js",
    "start-prod": "NODE_ENV=production ts-node-transpile-only ./src/server.ts",
    "doc": "apidoc -i src -o docs/api && apidoc-markdown -p docs/api -o docs/api.md",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "jest --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonprou/node-skeleton.git"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "assert"
    ],
    "ignore": [
      "docs"
    ]
  },
  "_moduleAliases": {
    "@constants": "./src/constants",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@utils": "./src/utils",
    "@services": "./src/services",
    "@controllers": "./src/controllers"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@ethersproject/abi": "^5.4.0",
    "@ethersproject/address": "^5.0.10",
    "@ethersproject/hash": "^5.0.11",
    "@ethersproject/providers": "^5.0.23",
    "async": "^2.6.1",
    "aws-sdk": "^2.405.0",
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cors": "2.7.1",
    "debug": "2.6.9",
    "express": "4.16.3",
    "express-async-errors": "^2.1.2",
    "express-jwt": "^5.3.1",
    "express-paginate": "^1.0.0",
    "express-validator": "^6.10.0",
    "methods": "1.1.2",
    "module-alias": "^2.1.0",
    "moment": "^2.23.0",
    "mongoose": "^5.11.17",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.10.0",
    "typescript": "^4.1.5",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "@rigwild/apidoc-markdown": "^2.0.2",
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.0",
    "@types/jest": "^26.0.20",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/redis": "^2.8.28",
    "@types/sinon": "^9.0.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "apidoc": "^0.17.7",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sinon": "^9.2.4",
    "standard": "^12.0.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1"
  }
}
